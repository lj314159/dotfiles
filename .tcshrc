set autolist
set noauto
set prompt = '%{\033[0;32m%}%~%{\033[0m%} '
setenv EDITOR vim
setenv VISUAL vim
setenv PAGER less
setenv CLICOLOR 1
setenv LSCOLORS ExFxBxDxCxegedabagacad
set history = 1000
set savehist = (1000 merge)
set histfile = ~/.tcsh_history
set autocorrect
set complete = enhance
alias setTitle 'printf "\033]0;%s\007" "$*:q"'
alias ls 'ls --color=auto'
alias stcsh 'source ~/.tcshrc'
setenv SHELL /usr/bin/tcsh
alias ch1 'cd /home/ubuntu/gameDev/cppGame/chapter1'
alias ch2 'cd /home/ubuntu/gameDev/cppGame/chapter2'
alias cpdot 'cp ~/.tcshrc ~/.bashrc ~/.vimrc ~/dotFiles/'
